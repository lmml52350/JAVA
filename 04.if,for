4.public class class04 {
    public static void main(String[] args) {
        String id = "901231-1234567";
        System.out.println(id.substring(0,8));
        System.out.println(id.substring(0,id.indexOf("-") + 2));

        String id2 = "234231-0654567";
        System.out.println(id2.substring(0,id2.indexOf("-") + 2));

        int hour = 10;
        if (hour < 14){
            System.out.println("아이스 아메리카노 +1");
            System.out.println("샷추가 +1");
        }
        System.out.println("주문완료");

        int hour2 = 12;//12시
        boolean moringcoffce = false;  //모닝커피 아직 안마심
        if (hour2 < 14 && !moringcoffce==false){  //! 반대
            System.out.println("아이스 아메리카노 +1");
        }

        if(hour2 >=14 || moringcoffce==true) {
            System.out.println("아이스 아메리카노 +2");
        }
        else {
            System.out.println("아이스 아메리카노 +3");
        }

        boolean hanlla = true;
        boolean mango = true;
        if(hanlla) {
            System.out.println("한라봉 +1");
        }
        else if (mango){
            System.out.println("망고 +1");
        }
        else{
            System.out.println("아이스 아메리카노 +3");
        }

        int sum = 0;
        for (int i =1; i<=10; i++){
            sum += 1;
            System.out.println("현재까지 총합은" + sum);
        }

    }
}
